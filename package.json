    "axios": "0.16.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "chalk": "^3.0.0",
    "concurrently": "3.5.0",
    "core-js": "^3",
    "css-loader": "^3.2.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "extract-css-chunks-webpack-plugin": "^4.6.0",
    "file-loader": "^4.2.0",
    "global": "^4.4.0",
    "html-webpack-plugin": "^4.3.0",
    "immer": "^8.0.1",
    "jest": "^25.1.0",
    "jest-transform-css": "^2.0.0",
