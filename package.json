{
  "name": "sanction-db",
  "version": "1.0.0",
  "description": "SanctionDB",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config ./config/webpack.config.js",
    "clean": "rm -rf build public",
    "test-dev": "jest --verbose --coverage --watch",
    "build": "webpack --config ./config/webpack.config.js"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^@src/(.*)$": "<rootDir>/src/$1",
      "^@client/(.*)$": "<rootDir>/src/client/$1",
      "^@static/(.*)$": "<rootDir>/src/client/static/$1",
      "^@styles/(.*)$": "<rootDir>/src/client/styles/$1",
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/client/utils/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.svg$": "jest-svg-transformer"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
